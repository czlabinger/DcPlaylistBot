<?xml version="1.0" encoding="utf-8"?>
<!-- This file contains project properties used by the build. -->
<Project>

  <!-- Plugin Metadata -->
  <PropertyGroup>
    <PluginId>DcPlaylistPlugin</PluginId>
    <PluginName>DcPlaylistPlugin</PluginName>
    <Authors>czlabinger</Authors>
    <Version>0.0.3</Version>
    <GameVersion>1.40.7</GameVersion>
    <Description>Add songs to playlist via discord</Description>
    <!--<PluginIcon></PluginIcon>-->
    <ProjectSource>https://github.com/czlabinger/DcPlaylistBot</ProjectSource>
    <ProjectHome>https://github.com/czlabinger/DcPlaylistBot</ProjectHome>
    <!--<DonateUrl></DonateUrl>-->
  </PropertyGroup>

  <!-- Plugin Dependencies -->
  <ItemGroup>
    <DependsOn Include="BSIPA" Version="^4.3.0"/>
  </ItemGroup>
  
  <PropertyGroup>
    <BSMTProjectType>BSIPA</BSMTProjectType>
    <GenerateManifest>true</GenerateManifest>
  </PropertyGroup>
  
  <PropertyGroup Condition="'$(Configuration)' != 'Release'">
    <DisableZipRelease>true</DisableZipRelease>
  </PropertyGroup>
  
</Project>